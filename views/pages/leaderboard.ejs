<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Leaderboard</title>
</head>
<body>
<h1>Leaderboard</h1>
<table>
    <thead>
    <tr>
        <th>Rank</th>
        <th>Pseudo</th>
        <th>Score</th>
        <th>Date</th>
    </tr>
    </thead>
    <tbody>
    <% topScores.forEach((score, index) => { %>
        <tr>
            <td><%= index + 1 %></td>
            <td><%= score.pseudo %></td>
            <td><%= score.score %></td>
            <td><%= new Date(score.date).toLocaleString() %></td>
        </tr>
    <% }) %>
    </tbody>
</table>
</body>
</html>
